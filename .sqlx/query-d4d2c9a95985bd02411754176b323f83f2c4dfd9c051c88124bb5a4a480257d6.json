{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT p.*, u.username AS author_username\n            FROM post p\n            LEFT JOIN users u ON p.author_id = u.id\n            WHERE p.parent_id = $1\n            ORDER BY p.created_at DESC\n            ",
